	name = Gabriel Sinkin
	email = gabriel.sinkin@gmail.com

[push]
    default = matching

[alias]
    cam = commit -am
    p = pull
    ci = commit -am
    rb = !sh -c 'git checkout --track -b $1 origin/$1'
    w = diff -M -w --color-words

[log]
    color = auto

[grep]
    color = auto

[apply]
    whitespace = strip

[diff]
    color = auto
    rename = copy

[pager]
    color = true

[status]
    color = auto

[color]
    ui = true
